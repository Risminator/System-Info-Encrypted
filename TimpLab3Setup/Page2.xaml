<Page x:Class="TimpLab3Setup.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TimpLab3Setup"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Download and Install Updates">

    <Grid>
        <TextBlock Text="Update has completed successfully. Please press 'Finish'."  Margin="30 30 0 0" />
        <Button x:Name="Btn2" Content="Finish"
                MinWidth="100" Width="100" MinHeight="20" Height="20"
                Click="Btn2_Click"
                VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0 0 50 10"/>
    </Grid>
</Page>
